{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build:parcel",
    "beforeDevCommand": "npm run dev:parcel",
    "devPath": "http://localhost:3000",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Ganymede",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "readFile": true,
        "writeFile": true,
        "renameFile": true,
        "readDir": true,
        "removeFile": true,
        "removeDir": true
      },
      "dialog": {
        "open": true,
        "confirm": true
      },
      "path": {
        "all": true
      },
      "shell": {
        "execute": true,
        "scope": [
          {
            "name": "recins",
            "cmd": "recins",
            "args": true
          },
          {
            "name": "recdel",
            "cmd": "recdel",
            "args": true
          },
          {
            "name": "read-macos-theme",
            "cmd": "defaults",
            "args": ["read", "-g", "AppleAccentColor"]
          }
        ]
      },
      "os": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.ganymede.ganymede",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 768,
        "resizable": true,
        "title": "Ganymede",
        "width": 1024
      }
    ]
  }
}
